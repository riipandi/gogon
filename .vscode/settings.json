{
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/*_templ.go": true
    },
    "search.exclude": {
        "**/build/*/**": true,
        "**/vendor/*/**": true
    },
    "files.watcherExclude": {
        "**/*_templ.go": true
    },
    "[templ]": {
        "editor.defaultFormatter": "a-h.templ"
    }
}
